{
  "name": "HNcute",
  "description": "make HN cute again",
  "version": "1.5",
  "manifest_version": 2,
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://news.ycombinator.com/*"]
  }],
  "web_accessible_resources": [
   "upvote.png",
   "hn-logo-cute.png",
   "OpenSans-Regular.ttf",
   "icon16.png"
 ],
 "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
